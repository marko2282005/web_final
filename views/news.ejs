<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>News</title>
</head>
<body>
  <h1>Latest News</h1>
  <ul>
    <% articles.forEach(article => { %>
      <li>
        <h3><%= article.title %></h3>
        <p><%= article.description %></p>
        <% if (article.url) { %>
          <a href="<%= article.url %>">Read more</a>
        <% } %>
      </li>
    <% }) %>
  </ul>
</body>
</html>
